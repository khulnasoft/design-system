<Kds::Button
  class={{this.classNames}}
  @size={{this.size}}
  @isFullWidth={{@isFullWidth}}
  @text={{@text}}
  @icon={{this.icon}}
  @isIconOnly={{@isIconOnly}}
  @color="secondary"
  @iconPosition="trailing"
  {{kds-clipboard text=@textToCopy target=@targetToCopy onSuccess=this.onSuccess onError=this.onError}}
  ...attributes
/>